    <xs:simpleType name="AddressPostalCodeType">
        <xs:annotation>
            <xs:documentation>A postal code that is not a US ZIP code.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="15"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AddressZIPCodeType">
        <xs:annotation>
            <xs:documentation>A US ZIP code.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="5"/>
            <xs:pattern value="\d{5}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AddressZIPExtensionCodeType">
        <xs:annotation>
            <xs:documentation>An extension of a US ZIP code.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="4"/>
            <xs:pattern value="\d{4}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ContactPhoneNumberType">
        <xs:annotation>
            <xs:documentation>Phone Number</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token">
            <xs:pattern value="\d{3}-\d{3}-\d{4}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ContactFaxNumberType">
        <xs:annotation>
            <xs:documentation>A facsimile number for a person or organization.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token">
            <xs:pattern value="\d{3}-\d{3}-\d{4}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ContactPhoneExtensionNumberType">
        <xs:annotation>
            <xs:documentation>A telephone suffix number to be entered to direct a call to the appropriate place.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token">
            <xs:maxLength value="9"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ContactEmailAddressTextType">
        <xs:annotation>
            <xs:documentation>An electronic e-mail address of a person or organization.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="48"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PersonSurNameType">
        <xs:annotation>
            <xs:documentation>A last name or family name of a person.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="PersonGivenNameType">
        <xs:annotation>
            <xs:documentation>A first name of a person.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="PersonSexCodeType">
        <xs:annotation>
            <xs:documentation>A code identifying the gender or sex of a person.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="1"/>
            <xs:enumeration value="F"/>
            <xs:enumeration value="M"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PersonMiddleNameType">
        <xs:annotation>
            <xs:documentation>A middle name of a person.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="PersonBirthDateType">
        <xs:annotation>
            <xs:documentation>The date of a person's birth.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
            <xs:pattern value="\d{4}-\d{2}-\d{2}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PersonSSNType">
        <xs:annotation>
            <xs:documentation>A person's social security number.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern value="\d{9}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PersonNameMatchCodeType">
        <xs:annotation>
            <xs:documentation>A code used to indicate what component(s) of the name matched the request. Values used for
                matching: 1 - Match to First, Middle, and Last Name sent; 2 - Match to 1st alternative name sent; 3 -
                Match to 2nd alternative name sent; No name match for SSN; 5 - Information not available for SSN sent.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="PersonFamilyViolenceIndicatorType">
        <xs:annotation>
            <xs:documentation>A Y=Yes or N=No value that indicates whether famliy violence exists for a person
                participating in a case.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="1"/>
            <xs:enumeration value="Y"/>
            <xs:enumeration value="N"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="OrganizationAgencyCodeType">
        <xs:annotation>
            <xs:documentation>A code that identifies a Federal Agency.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:token">
            <xs:enumeration value="A03"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="OrganizationNameType">
        <xs:annotation>
            <xs:documentation>The name of an organization.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="SystemUserIdentifierType">
        <xs:annotation>
            <xs:documentation>An identifier that associates a user with access or authorization to a system.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="SystemServerIdentifierType">
        <xs:annotation>
            <xs:documentation>An identifier that uniquely defines a Server Address within a network.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="StateTribeTerritoryNameType">
        <xs:annotation>
            <xs:documentation>The full name of a state, tribe or territory.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="EndDateType">
        <xs:annotation>
            <xs:documentation>The ending or finishing point in a date range.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
            <xs:pattern value="\d{4}-\d{2}-\d{2}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StartDateType">
        <xs:annotation>
            <xs:documentation>The beginning or starting point in a date range.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
            <xs:pattern value="\d{4}-\d{2}-\d{2}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AppliedDateType">
        <xs:annotation>
            <xs:documentation>The date an event was applied to a case or an account.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
            <xs:pattern value="\d{4}-\d{2}-\d{2}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AppliedAmountType">
        <xs:annotation>
            <xs:documentation>A money amount applied to a case or an account.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:totalDigits value="11"/>
            <xs:fractionDigits value="2"/>
            <xs:pattern value="\d{1,9}\.\d{2}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PaymentLastIndicatorType">
        <xs:annotation>
            <xs:documentation>Last Payment Indicator - Valid values are Y or N. This indicator identifies the Last
                Payment received for a case. There should be only one Y per payment group. This element maps to Last
                Payment Date and Last Payment Amount.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Y"/>
            <xs:enumeration value="N"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PaymentSourceCodeType">
        <xs:annotation>
            <xs:documentation>Payment Source Code - Valid Codes: FTO=IRS Tax Intercept; UBI=Unemployment Benefit
                Insurance Intercept; WCI=Workers Compensation; STO=State Tax Intercept; AEI=Administrative Enforcement
                for Interstate Cases; NDP=Received directly from NCP; NIW=Income Withholding; FAO=Administrative Offset;
                IFC=IRS Collection; FID=Financial Institution Data Match Levy; PDC=Passport Denial Collection; OTH=Other
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="3"/>
            <xs:enumeration value="FTO"/>
            <xs:enumeration value="UBI"/>
            <xs:enumeration value="WCI"/>
            <xs:enumeration value="STO"/>
            <xs:enumeration value="AEI"/>
            <xs:enumeration value="NDP"/>
            <xs:enumeration value="NIW"/>
            <xs:enumeration value="FAO"/>
            <xs:enumeration value="IFC"/>
            <xs:enumeration value="FID"/>
            <xs:enumeration value="PDC"/>
            <xs:enumeration value="OTH"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="RecipientTypeCodeType">
        <xs:annotation>
            <xs:documentation>Disbursement Recipient Type Code Valid Codes: P=Custodial Parent; T=TANF; F=FosterCare;
                O=Other; S=State
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="1"/>
            <xs:enumeration value="P"/>
            <xs:enumeration value="T"/>
            <xs:enumeration value="F"/>
            <xs:enumeration value="O"/>
            <xs:enumeration value="S"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="RecipientNameType">
        <xs:annotation>
            <xs:documentation>Disbursement Recipient Name</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="InstrumentNumberType">
        <xs:annotation>
            <xs:documentation>Disbursement Instrument Number</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="15"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="RequestDateTimeType">
        <xs:annotation>
            <xs:documentation>The date/time that a request for information was made.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:pattern
                    value="\d{4}-((0[1-9])|(1[0-2]))-((0[1-9])|([1-2][0-9])|(3[0-1]))T(([0-1][0-9])|(2[0-4])):[0-5][0-9]:[0-5][0-9](\.\d{3})?"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="RequestorIdentifierType">
        <xs:annotation>
            <xs:documentation>A unique identifier for a request. First 2 characters are a state number followed by 3
                additional characters (suggest 000).
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="5"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StateGeneratedIdentifierType">
        <xs:annotation>
            <xs:documentation>A unique identifier supplied by a state to associate a case query to a specific request.
                (State could use a date/timestamp for this element).
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="20"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ObligationSupportAmountType">
        <xs:annotation>
            <xs:documentation>Obligation Support Amount</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:totalDigits value="11"/>
            <xs:fractionDigits value="2"/>
            <xs:pattern value="\d{1,9}\.\d{2}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ObligationArrearsAmountType">
        <xs:annotation>
            <xs:documentation>Obligation Arrears Amount</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:totalDigits value="11"/>
            <xs:fractionDigits value="2"/>
            <xs:pattern value="\d{1,9}\.\d{2}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ObligationOtherAmountType">
