<xs:schema xmlns:idy="urn:us:gov:hhs:acf:idy" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cse="urn:us:gov:hhs:acf:cse" targetNamespace="urn:us:gov:hhs:acf:idy" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="urn:us:gov:hhs:acf:cse" schemaLocation="cse.xsd"/>
	<xs:element name="IdentifyingInfoRoot">
		<xs:annotation>
			<xs:documentation>This is the root element for the Case Indentifying Information.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="IdentifyingInfo" type="cse:CaseIdentifyingInfoType">
